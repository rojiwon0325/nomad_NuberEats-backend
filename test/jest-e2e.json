{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },"moduleNameMapper":{
    "^src/(.*)$":"<rootDir>/../src/$1",
    "^@global/(.*)$":"<rootDir>/../src/global/$1",
    "^@user/(.*)$":"<rootDir>/../src/user/$1",
    "^@jwt/(.*)$":"<rootDir>/../src/jwt/$1",
    "^@auth/(.*)$":"<rootDir>/../src/auth/$1",
    "^@mail/(.*)$":"<rootDir>/../src/mail/$1"
  }
}
